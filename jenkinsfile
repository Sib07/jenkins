pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
     
        stage('saba') {
            steps {
                echo 'Hello World saba'
            }
        }
     
        stage('aimen') {
            steps {
                echo 'Hello World aimen'
            }
        }
        
    }
}
